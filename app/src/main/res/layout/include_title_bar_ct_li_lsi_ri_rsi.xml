<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:lyman="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/title_bar_bg"
    android:orientation="vertical">

    <View
        android:id="@+id/paddingView"
        android:layout_width="match_parent"
        android:layout_height="0dp" />

    <ly.customselectitem.CustomSelectItem
        android:id="@+id/cusTitleBar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_bar_height"
        android:background="@color/title_bar_bg"
        lyman:centerTextColorSelect="@color/white"
        lyman:centerTextSelect="标题"
        lyman:centerTextSizeSelect="@dimen/text_size_big02"
        lyman:centerTypeSelect="text"

        lyman:leftImagePaddingLeftSelect="@dimen/title_bar_padding_left"
        lyman:leftImageSourceSelect="@drawable/icon_white_back"
        lyman:leftTypeSelect="image"

        lyman:leftSideTypeSelect="image"
        lyman:leftSideImageSourceSelect="@drawable/icon_white_screen"
        lyman:leftSideImagePaddingLeftSelect="20dp"

        lyman:rightTypeSelect="image"
        lyman:rightImageSourceSelect="@drawable/icon_white_screen"
        lyman:rightImagePaddingRightSelect="@dimen/title_bar_padding_right"

        lyman:rightSideTypeSelect="image"
        lyman:rightSideImageSourceSelect="@drawable/icon_white_screen"
        lyman:rightSideImagePaddingRightSelect="20dp"
        />
</LinearLayout>